programa
{
	
	funcao inicio()
	{
		inteiro contador,limite,resultado,multi

		contador = 0
		

		escreva ("Defina o multiplicando da tabuada que deseja: ")
		leia (multi)
		escreva ("Defina o limite da tabuada: ")
		leia (limite)
		

		faca{

			resultado = multi * contador
			escreva(multi + " X " + contador + " = " + resultado + "\n")
			contador ++
			
		}enquanto (contador<=limite)
		
	}
}
